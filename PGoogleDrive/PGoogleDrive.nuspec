<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>PGoogleDrive</id>
    <version>1.0.0</version>
    <title>PGoogleDrive library for google drive api</title>
    <authors>Posled Nikita</authors>
    <owners>Posled Nikita</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Google APIs Client Library for working with Drive v3.

      Supported Platforms:
      - .NET Framework 4.5+
      - .NET Standard 1.3 and .NET Standard 2.0; providing .NET Core support.

      Legacy platforms:
      - .NET Framework 4.0
      - Windows 8 Apps
      - Windows Phone 8.1
      - Windows Phone Silverlight 8.0

      Incompatible platforms:
      - .NET Framework &lt; 4.0
      - Silverlight
      - UWP (will build, but is known not to work at runtime)
      - Xamarin

</description>
    <copyright></copyright>
    <tags>Google Drive v3 API</tags>
    <repository type="git" url="https://github.com/R1kNk/PGDrive" />
    <dependencies>
        <dependency id="Google.Apis.Drive.v3" version="1.36.1.1372" />
    </dependencies>
  </metadata>
</package>